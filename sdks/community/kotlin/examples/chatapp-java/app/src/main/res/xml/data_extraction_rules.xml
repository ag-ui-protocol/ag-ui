<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude sensitive data from cloud backup -->
        <exclude domain="sharedpref" path="agent_settings.xml"/>
    </cloud-backup>
    <device-transfer>
        <!-- Include app data in device transfer -->
        <include domain="sharedpref" path="."/>
    </device-transfer>
</data-extraction-rules>