  "dependencies": {
    "rxjs": "7.8.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "publint": "^0.3.12",
    "@arethetypeswrong/cli": "^0.17.4",
    "ts-jest": "^29.1.2",
    "tsdown": "^0.20.1",
    "typescript": "^5.8.2"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
