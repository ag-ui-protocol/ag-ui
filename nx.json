{
  "$schema": "https://nx.dev/reference/nx-json#workspace-configuration",
  "useDaemonProcess": true,
  "parallel": 30,
  "targetDefaults": {
    "generate": {
      "outputs": [
        "{projectRoot}/src/generated"
      ],
      "cache": true
    },
    "build": {
      "dependsOn": [
        "^build",
        "generate"
      ],
      "inputs": [
        "{projectRoot}/**/*",
        "{projectRoot}/.env*"
      ],
      "outputs": [
        "{projectRoot}/dist",
        "{projectRoot}/.next"
      ],
      "cache": true
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ],
      "cache": true
    },
    "check-types": {
      "dependsOn": [
        "^check-types"
      ],
      "cache": true
    },
    "dev": {
      "dependsOn": [
        "^build",
        "generate"
      ],
      "cache": false
    },
    "test": {
      "dependsOn": [
        "build"
      ],
      "cache": true
    },
    "test:coverage": {
      "dependsOn": [
        "build"
      ],
      "outputs": [
        "{projectRoot}/coverage"
      ],
      "cache": true
    },
    "test:watch": {
      "dependsOn": [
        "build"
      ],
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "link:global": {
      "cache": false
    },
    "unlink:global": {
      "cache": false
    },
    "start": {
      "dependsOn": [
        "^build"
      ],
      "cache": false
    }
  }
}